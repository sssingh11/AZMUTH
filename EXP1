
# Create Variables and Display their values
num1=10
num2=20
print(num1,num2)
# Apply arithmetic operations and display results
add=num1+num2
sub=num1-num2
mul=num1*num2
div=num1/num2
print("RESULTS")
print(add,sub,mul,div)
print(num1,'+',num2, '=',add)
print(num1,'-',num2, '=',sub)
print(num1,'*',num2, '=',mul)
print(num1,'/',num2, '=',div)

